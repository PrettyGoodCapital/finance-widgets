{
  "name": "@finance-widgets/core",
  "version": "0.1.0",
  "description": "Core finance widgets",
  "repository": "git@github.com:prettygoodcapital/finance-widgets.git",
  "author": "prettygoodcapital <prettygoodcapital@gmail.com>",
  "license": "Apache-2.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "browser": "dist/index.js",
  "unpkg": "dist/index.js",
  "jsdelivr": "dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "scripts": {
    "build:esbuild": "node build.js",
    "build:tsc": "tsc",
    "build": "npm-run-all -p build:*",
    "watch:esbuild": "yarn build:esbuild --watch",
    "watch": "npm-run-all -p watch:*",
    "clean": "rimraf dist lib coverage junit.xml",
    "fix": "yarn lint --fix",
    "lint": "eslint --ext js --ext ts src/ build.js",
    "test": "NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --config .jestrc.js --forceExit --coverage"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {}
}
